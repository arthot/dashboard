{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn",
        "quotes": ["warn", "single"]
    }
}