    "express": "^4.16.4",
    "express-bunyan-logger": "^1.3.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.2",
    "lodash": "^4.17.11",
    "nconf": "^0.10.0",
    "nocache": "^2.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.2.0",
    "sqlite3": "^4.0.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
